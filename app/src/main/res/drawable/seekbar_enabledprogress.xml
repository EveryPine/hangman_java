<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 시크바 배경 -->
    <item android:id="@+id/background">
        <shape android:shape="rectangle">
            <!--시크바 선 굵기: 길이 10dp -->
            <stroke android:width="6dp" android:color="#8DC587"/>
            <!-- 시크바 내용 색상 -->
            <solid android:color="#8DC587"/>
            <!-- 테두리 둥글게 설정 -->
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <item android:id="@+id/progress">
        <clip>
            <shape android:shape="rectangle">
                <!--시크바 선 굵기: 길이 10dp -->
                <stroke android:width="6dp" android:color="#16961D"/>
                <!-- 시크바 내용 색상 -->
                <solid android:color="#16961D"/>
                <!-- 테두리 둥글게 설정 -->
                <corners android:radius="10dp"/>
            </shape>
        </clip>
    </item>

</layer-list>
